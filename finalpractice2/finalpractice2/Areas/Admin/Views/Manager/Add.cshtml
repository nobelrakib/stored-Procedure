
@model finalpractice2.Areas.Admin.Models.ManagerUpdateModel
@{
    ViewData["Title"] = "Add Manager";
}

@section notification
{
    @await Html.PartialAsync("_Notification", Model.Notification)
}
@using (Html.BeginForm("Add", "Manager", FormMethod.Post, new { @class = "form-style" }))
{
    @Html.AntiForgeryToken()
    <h1>Test Form</h1>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(model => model.Name, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.Name, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Name, "Email is required", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" class="btn btn-default" value="Submit" />
        </div>
    </div>
}